{
  "name": "generate-passphrase",
  "version": "1.0.0",
  "description": "Zero dependency module - Secure random passphrase",
  "main": "build/index.cjs",
  "module": "build/index.js",
  "types": "build/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.cjs"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "files": [
    "build/index.cjs",
    "build/index.js",
    "build/words.txt",
    "build/src/index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.ts --fix --ignore-path .gitignore .",
    "coverage": "nyc yarn run uvu",
    "uvu": "uvu -r ts-node/register test",
    "pretest": "rimraf build && yarn run lint && yarn run build",
    "test": "yarn run coverage",
    "benchmark": "node benchmark/benchmark"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldy505/generate-passphrase.git"
  },
  "keywords": [
    "passphrase",
    "generate",
    "secure",
    "random"
  ],
  "author": "Reinaldy Rafli <aldy505@tutanota.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aldy505/generate-passphrase/issues"
  },
  "homepage": "https://github.com/aldy505/generate-passphrase#readme",
  "engines": {
    "node": ">=12"
  },
  "directories": {
    "test": "./test",
    "lib": "./src"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@types/node": "15.12.1",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "benchmark": "2.1.4",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.28.0",
    "eslint-config-xo-space": "^0.27.0",
    "generate-password": "1.6.0",
    "microtime": "3.0.0",
    "niceware": "2.0.2",
    "nyc": "15.1.0",
    "randomatic": "3.1.1",
    "rimraf": "3.0.2",
    "rollup": "2.51.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "source-map-support": "0.5.19",
    "ts-node": "10.0.0",
    "tslib": "2.2.0",
    "typescript": "4.3.2",
    "uvu": "0.5.1"
  }
}
