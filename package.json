{
  "name": "generate-passphrase",
  "version": "1.0.0",
  "description": "Zero dependency module - Secure random passphrase",
  "main": "build/index.cjs",
  "module": "build/index.js",
  "types": "build/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.cjs"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "files": [
    "build/index.cjs",
    "build/index.js",
    "build/words.txt",
    "build/src/index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ignore-path .gitignore .",
    "coverage": "nyc npm run mocha",
    "mocha": "mocha -r ts-node/register test/**/*.spec.ts --exit",
    "pretest": "rimraf build && npm run lint && npm run build",
    "test": "npm run coverage",
    "benchmark": "node benchmark/benchmark"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldy505/generate-passphrase.git"
  },
  "keywords": [
    "passphrase",
    "generate",
    "secure",
    "random"
  ],
  "author": "Reinaldy Rafli <reinaldyrfl@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aldy505/generate-passphrase/issues"
  },
  "homepage": "https://github.com/aldy505/generate-passphrase#readme",
  "engines": {
    "node": ">=10"
  },
  "directories": {
    "test": "./test",
    "lib": "./src"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@types/chai": "4.2.18",
    "@types/mocha": "8.2.2",
    "@types/node": "15.0.2",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "benchmark": "2.1.4",
    "chai": "4.3.4",
    "eslint": "7.26.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "generate-password": "1.6.0",
    "microtime": "3.0.0",
    "mocha": "8.4.0",
    "niceware": "2.0.2",
    "nyc": "15.1.0",
    "randomatic": "3.1.1",
    "rimraf": "3.0.2",
    "rollup": "2.47.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "source-map-support": "0.5.19",
    "ts-node": "9.1.1",
    "tslib": "2.2.0",
    "typescript": "4.2.4"
  }
}
